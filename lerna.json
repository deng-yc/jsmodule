{
    "packages": [
        "shared/*",
        "packages/*"
    ],
    "command": {
        "publish": {
            "ignoreChanges": [
                "ignored-file",
                "*.md"
            ],
            "message": "chore(release): publish",
            "registry": "https://npm.pkg.github.com"
        },
        "build": {
            "ignore": "examples/*",
            "npmClientArgs": []
        }
    },
    "useWorkspaces": true,
    "version": "2.0.1-alpha.0",
    "npmClient": "yarn"
}